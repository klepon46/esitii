<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ggstudio.esitii">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_robo_lauch"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/MainTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.RegulasiActivity"
            android:parentActivityName=".activities.RegulasiListActivity" />
        <activity
            android:name=".activities.PanduanActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activities.VideoActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MainTheme" />
        <activity
            android:name=".activities.RegulasiListActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activities.VideoOldActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activities.ReadMoreActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activities.ContactUsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activities.RegulasiNewActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MainTheme" />
        <activity
            android:name=".activities.WebViewActivity"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>